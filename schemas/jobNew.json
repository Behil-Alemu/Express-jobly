{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/jobNew.json",
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "minLength": 1,
        "maxLength": 30
      },
      "title": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      },
      "salary": {
        "type": "integer",
        "minLength": 1,
        "maxLength": 30
      },
      "equity": {
        "type": "number",
        "minLength": 1,
        "maxLength": 30
      },
      "companyHandle": {
        "type": "string",
        "minLength": 0,
        "maxLength": 60
      }
    },
    "additionalProperties": false,
    "required": [
      "id",
      "title",
      "salary",
      "equity",
      "companyHandle"
    ]
  }
  